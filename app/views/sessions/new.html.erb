<h1>Sign in</h1>
<p id="notice"><%= notice %></p>
<%= form_tag sessions_path do %>
    <%= label_tag(:q, "Username: ") %>
    <%= text_field_tag :username, params[:username] %>
    <%= label_tag(:q, "Password: ") %>
    <%= password_field_tag :password, params[:password] %>
    <%= submit_tag "Log in" %>
<% end %>